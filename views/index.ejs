<!DOCTYPE html>
<html lang="pt-br">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Guia perguntas</title>
</head>
<body>
  <h1>Guia perguntas</h1>
  <hr>
  <h3>Lista perguntas</h3>
  <p>
    Nome: <%= name %>
  </p>
  <p>
    Linguagem: <%= lang %>
  </p>
  <% if(exibirMsg) { %>
    <h3>Isso e uma mensagem de erro</h3>    
  <% } else { %>
    <h3>Nenhum erro</h3>
  <% } %>
  
  <% produtos.forEach(function(produto) { %>
    <hr>
    <h3>
      <%= produto.name %>
    </h3>
    <h4>
      <%= produto.price %>
    </h4>


  <% }) %>
</body>
</html>